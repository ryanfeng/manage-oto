<!--服务器设置-->
<table class="input-form mt20">
    <template v-if="list">
        <tbody>
        <template v-for="val in list">
            <tr class="{{val.class}}">
                <th>{{val.title}}</th>
                <td>
                    <input type="{{val.type}}" class="input input-long {{val.warning&&'warning'}}" v-on:keyup.enter="submit" disabled="{{val.disabled||false}}" v-on:blur="val.blur(val,list,channel)" v-model="val.value" type="text" placeholder="{{val.placeholder}}" value="{{val.value}}"/>
                    <div class="hint">{{val.hint}}</div>
                </td>
            </tr>
        </template>
        <tr>
            <th></th>
            <td> 
                <div class="mt20">
                    <a href="javascript:void(0)" class="btn {{submitClass}} mr5" v-on:click="submit">{{submitValue}}</a>
                    <a href="./school-device.html" class="btn {{cancelClass}} mr5">{{cancelValue}}</a>
                </div>
            </td>
        </tr>
        </tbody>
    </template>
</table>