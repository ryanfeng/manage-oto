<!--滤网列表-->
<table class="info-table">
    <thead>
    <tr>
        <template v-for="val in titles">
            <th v-bind:style="val.styleObject">
                <template v-if="val.select">
                    <!--省份-->
                    <div class="sod_select">
                        <div class="sod_label">{{val.active}}</div>
                        <div class="sod_list">
                            <ul>
                                <li v-for="option in val.options" v-on:click="val.click(option,val)" class="{{option.class}}">{{option.channelName}}</li>
                            </ul>
                        </div> 
                    </div>  
                </template>
                <template v-else>
                    {{val.text}}
                </template>
            </th>
        </template>
    </tr>
    </thead>
    <tbody>
    <template v-for="val in dataList">
        <tr>
            <td>{{$index+1+page}}</td>
            <td>{{val.modelName}}</td>
            <td>{{val.sn}}</td>
            <td>{{val.deviceView&&val.deviceView.modelName}}</td>
            <td>{{val.deviceView&&val.deviceView.sn}}</td>
            <td>{{val.remainingLife}}</td>
            <td class="long">{{val.deviceView&&val.deviceView.fullAdderss}}</td>
            <td>{{{val.deviceView&&val.deviceView.deviceStatus}}}</td> 
            <td>{{val.deviceView&&val.deviceView.isConnect==0?"否":"是"}}</td>
            <td>{{val.deviceView&&val.deviceView.channelName}}</td>
            <td>{{val.deviceView&&val.deviceView.appUserPhone}}</td>
            <td class="tools"> 
                <ul>  
                    <li>
                        <a href="./strainer-detail.html?strainerId={{val.strainerId}}&deviceId={{(val.deviceView&&val.deviceView.deviceId)||''}}" >{{val.tools.detail}}</a>
                    </li>  
                </ul>
            </td>
        </tr>
    </template>
    </tbody>
</table>