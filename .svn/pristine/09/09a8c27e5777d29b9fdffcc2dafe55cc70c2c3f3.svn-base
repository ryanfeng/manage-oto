<!--设备型号列表-->
<table class="info-table">
    <thead>
    <tr>
        <template v-for="val in titles">
            <th v-bind:style="val.styleObject">
                <template v-if="val.select">
                    <!--省份-->
                    <div class="sod_select">
                        <div class="sod_label">{{val.active}}</div>
                        <div class="sod_list">
                            <ul>
                                <li v-for="option in val.options" v-on:click="val.click(option,val)" class="{{option.class}}">{{option.channelName}}</li>
                            </ul>
                        </div> 
                    </div>  
                </template>
                <template v-else>
                    {{val.text}}
                </template>
            </th>
        </template>
    </tr>
    </thead>
    <tbody>
    <template v-for="val in dataList">
        <tr>
            <td>{{$index+1+page}}</td>
            <td>{{val.modelName}}</td>
            <td>{{val.sn}}</td>
            <td>{{val.strainerView.modelName}}</td>
            <td>{{val.strainerView.sn}}</td>
            <td>{{val.strainerView.remainingLife}}</td>
            <td class="long">{{val.fullAdderss}}</td>
            <td>{{{val.deviceStatus}}}</td>
            <td>{{val.isConnect==0?"否":"是"}}</td>
            <td>{{val.channelName}}</td>
            <td>{{val.appUserPhone}}</td>
            <td class="tools"> 
                <ul>
                    <li>
                        <a href="./device-detail.html?deviceId={{val.deviceId}}&strainerId={{val.strainerView.strainerId}}">{{val.tools.detail}}</a>
                    </li>  
                </ul>
            </td>
        </tr>
    </template>
    </tbody>
</table>