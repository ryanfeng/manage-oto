<!--查看经销商设备-->
<table class="info-table">
    <thead>
    <tr>
        <template v-for="val in titles">
            <th v-bind:style="val.styleObject">
                <template v-if="val.select">
                    <!--省份-->
                    <div class="sod_select">
                        <div class="sod_label">{{val.active}}</div>
                        <div class="sod_list">
                            <ul>
                                <li v-for="option in val.options" v-on:click="val.click(option.isEnable,val)" class="{{option.class}}">{{option.text}}</li>
                            </ul>
                        </div>
                    </div>
                </template>
                <template v-else>
                    {{val.text}}
                </template>
            </th>
        </template>
    </tr>
    </thead>
    <tbody>
    <template v-for="val in dataList">
        <tr>
            <td>{{$index+1+page}}</td>
            <td>{{val.nickName}}</td>
            <td>{{val.modelName}}</td>
            <td>{{val.factoryName}}</td>
            <td>{{val.sn}}</td>
        </tr>
    </template>
    <template v-if="dataList.length==0">
        <tr class="tc">
            <td colspan="5"><b>没有数据</b></td>
        </tr>
    </template>
    </tbody>
</table>